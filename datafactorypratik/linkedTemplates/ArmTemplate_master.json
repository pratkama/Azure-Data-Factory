{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorypratik"
		},
		"Maheer_Demo_Linked_Servuce_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Maheer_Demo_Linked_Servuce'"
		},
		"conn_pratik_sql_test_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conn_pratik_sql_test_db'"
		},
		"datalake_gen_2_connection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake_gen_2_connection'"
		},
		"datalake_synapse_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'datalake_synapse_connection'"
		},
		"emp_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'emp_blob'"
		},
		"temp_staging_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'temp_staging'"
		},
		"linkedservicesql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkedservicesql'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakepratikdp203.dfs.core.windows.net/"
		},
		"datalake_gen_2_connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakepratikdp203.dfs.core.windows.net/"
		},
		"temp_staging_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakepratikdp203.dfs.core.windows.net/"
		},
		"linkedservicesql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "pratiksqldb.database.windows.net"
		},
		"linkedservicesql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().dbname}"
		},
		"linkedservicesql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "pratiksqldb"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorypratik_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Maheer_Demo_Linked_Servuce_connectionString": {
						"value": "[parameters('Maheer_Demo_Linked_Servuce_connectionString')]"
					},
					"conn_pratik_sql_test_db_connectionString": {
						"value": "[parameters('conn_pratik_sql_test_db_connectionString')]"
					},
					"datalake_gen_2_connection_accountKey": {
						"value": "[parameters('datalake_gen_2_connection_accountKey')]"
					},
					"datalake_synapse_connection_connectionString": {
						"value": "[parameters('datalake_synapse_connection_connectionString')]"
					},
					"emp_blob_connectionString": {
						"value": "[parameters('emp_blob_connectionString')]"
					},
					"temp_staging_accountKey": {
						"value": "[parameters('temp_staging_accountKey')]"
					},
					"linkedservicesql_password": {
						"value": "[parameters('linkedservicesql_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"datalake_gen_2_connection_properties_typeProperties_url": {
						"value": "[parameters('datalake_gen_2_connection_properties_typeProperties_url')]"
					},
					"temp_staging_properties_typeProperties_url": {
						"value": "[parameters('temp_staging_properties_typeProperties_url')]"
					},
					"linkedservicesql_properties_typeProperties_server": {
						"value": "[parameters('linkedservicesql_properties_typeProperties_server')]"
					},
					"linkedservicesql_properties_typeProperties_database": {
						"value": "[parameters('linkedservicesql_properties_typeProperties_database')]"
					},
					"linkedservicesql_properties_typeProperties_userName": {
						"value": "[parameters('linkedservicesql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datafactorypratik_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactorypratik_ArmTemplate_0')]"
			]
		},
		{
			"name": "datafactorypratik_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactorypratik_ArmTemplate_1')]"
			]
		}
	]
}